{
  "name": "meowlink",
  "version": "0.1.0",
  "description": "üê± MeowLink - Knowledge Collection App for Developers",
  "main": "main.js",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dev": "npm run build-react && electron . --dev",
    "build": "npm run build-react && electron-builder",
    "build-react": "webpack --mode=production",
    "build-react-dev": "webpack --mode=development --watch",
    "build-mac": "npm run build-react && electron-builder --mac --publish=never",
    "build-win": "npm run build-react && electron-builder --win --publish=never",
    "build-linux": "npm run build-react && electron-builder --linux --publish=never",
    "pack": "npm run build-react && electron-builder --dir",
    "clean": "rm -rf node_modules package-lock.json dist build",
    "rebuild": "npm rebuild --runtime=electron --target=13.6.9 --disturl=https://atom.io/download/electron --build-from-source"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axios": "0.24.0",
    "cheerio": "1.0.0-rc.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "5.0.11",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "electron": "13.6.9",
    "electron-builder": "22.14.13",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.9.3",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.meowlink.app",
    "productName": "MeowLink",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "build/**/*",
      "renderer/**/*",
      "utils/**/*",
      "assets/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "assets/icon.icns",
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "title": "Install MeowLink",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
